<?php
namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
* @ORM\Entity
* @ORM\Table(name="developper")
*/
class Developper {
  /**
  * @ORM\Column(type="integer")
  * @ORM\Id
  * @ORM\GeneratedValue(strategy="AUTO")
  */
  private $id;
  /**
  * @ORM\Column(type="string", length=100)
  */
  private $username;
  /**
  * @ORM\Column(type="string", length=100)
  */
  private $title;
  /**
  * @ORM\Column(type="text")
  */
  private $description;
  /**
  * @ORM\Column(type="string", length=100)
  */
  private $github;
  /**
  * @ORM\Column(type="text")
  */
  private $languages;
}

?>
